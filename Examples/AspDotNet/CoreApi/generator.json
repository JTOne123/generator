{
  "version": 2,
  "output": "..\\..\\Angular\\src\\app",
  "generate": [
    {
      "read": "asp",
      "controller": {
        "assembly": "KY.Generator.Examples.AspDotNetCore",
        "namespace": "KY.Generator.Examples.AspDotNetCore.Controllers",
        "name": "ValuesController"
      }
    },
    {
      "write": "angular",
      "service": {
        "relativePath": "services",
        "name": "ValuesCoreService"
      },
      "model": {
        "relativePath": "models",
        "propertiesToFields": true
      }
    }
  ]
}